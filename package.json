{
  "name": "react-graphql-apollo",
  "version": "1.0.0",
  "description": "graphql server & react apollo client",
  "main": "graphql/server.js",
  "scripts": {
    "server": "node graphql/server.js",
    "client": "npm start --prefix client",
    "api": "json-server api/generate.js --port 5001",
    "dev": "concurrently \"npm run api\" \"npm run server\" \"npm run client\""
  },
  "author": "rajiv ranjan",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "faker": "^4.1.0",
    "graphql": "^14.2.0",
    "json-server": "^0.14.2",
    "lodash": "^4.17.11"
  }
}
